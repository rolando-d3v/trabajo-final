generator client {
  provider = "prisma-client-js"
   output   = "./generated/client1"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model clasificacion_doc {
  ID_CLASIFI_DOC_I Int         @id @default(autoincrement())
  DESC_CORTA_V     String      @db.VarChar(100)
  DESC_LARGA_V     String      @db.VarChar(200)
  ESTADO_B         Boolean?    @default(dbgenerated("(true)"))
}

model documento {
  ID_CLASIFI_DOC_I Int         @id @default(autoincrement())
  DESC_CORTA_V     String      @db.VarChar(100)
  DESC_LARGA_V     String      @db.VarChar(200)
  ESTADO_B         Boolean?    @default(dbgenerated("(true)"))
}

